{
  "addons": [
    "mongolab",
    "rediscloud"
  ],
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    }
  ],
  "env": {
    "JWT_SECRET": {
      "required": true
    },
    "MOL_CACHER": {
      "required": true
    },
    "MOL_CACHER_URL": {
      "required": true
    },
    "MOL_TRANSPORTER": {
      "required": true
    },
    "MONGO_URI": {
      "required": true
    },
    "MONGO_URI_URI": {
      "required": true
    },
    "NPM_CONFIG_PRODUCTION": {
      "required": true
    },
    "YARN_PRODUCTION": {
      "required": true
    }
  },
  "formation": {
    "web": {
      "quantity": 1
    }
  },
  "name": "guru-erp",
  "scripts": {
  },
  "stack": "heroku-18"
}
