{
  "extends": "tslint-config-airbnb",
  "rules": {
    "semicolon": false,
    "trailing-comma": false,
    "eofline": {
      "severity": "warn"
    },
    "no-consecutive-blank-lines": {
      "severity": "warn"
    },
    "quotemark": {
      "severity": "warn",
      "options": ["single", "jsx-double"]
    },
    "no-trailing-whitespace": {
      "severity": "warn"
    },
    "ordered-imports": {
      "severity": "warn"
    },
    "import-name": [
      true,
      {
        "react": "React"
      }
    ],
    "align": {
      "severity": "warn"
    }
  }
}
